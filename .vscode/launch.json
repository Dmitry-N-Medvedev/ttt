{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "LibMatrix",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sources/libs/libt3/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}/sources/libs/libt3",
      "args": [
        "--spec",
        "specs/LibMatrix.spec.mjs"
      ],
      "env": {
        "NODE_ENV": "testing"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LibBlockerStrategy",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sources/libs/libt3/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}/sources/libs/libt3",
      "args": [
        "--spec",
        "specs/LibBlockerStrategy.spec.mjs"
      ],
      "env": {
        "NODE_ENV": "testing"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LibArbiter",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sources/libs/libt3/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}/sources/libs/libt3",
      "args": [
        "--timeout",
        "0",
        "--spec",
        "specs/LibArbiter.spec.mjs"
      ],
      "env": {
        "NODE_ENV": "testing"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "LibT3",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sources/libs/libt3/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}/sources/libs/libt3",
      "args": [
        "--allow-uncaught",
        "--async-only",
        "--bail",
        "--check-leaks",
        "--exit",
        "--forbid-only",
        "--forbid-pending",
        "--slow",
        "50",
        "--timeout",
        "1000",
        "--color",
        "--diff",
        "--full-trace",
        "--inline-diffs",
        "--reporter",
        "spec",
        "--no-config",
        "--no-opts",
        "--spec",
        "./specs/LibT3.spec.mjs"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "testing"
      }
    }
  ]
}